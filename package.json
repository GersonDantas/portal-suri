  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/main/**/*",
      "!<rootDir>/src/domain/**/*",
      "!<rootDir>/src/__tests__/**/*",
      "!<rootDir>/src/**/index.{ts,js,tsx}",
      "!<rootDir>/**/*.d.ts"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(s?)css$": "identity-obj-proxy",
      "src/(.*)": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)+(spec).{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
