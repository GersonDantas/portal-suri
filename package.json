{
  "name": "portal-suri",
  "version": "1.0.0",
  "description": "This project is for testing chatbotmaker platform with news patterns",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "ionic serve",
    "build": "ionic build",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)' --passWithNoTests --no-cache --runInBand",
    "eject": "react-scripts eject",
    "test:all": "npm run test -- --watchAll",
    "test:staged": "npm run test -- --watchAll=false --env=jsdom --findRelatedTests",
    "test:ci": "npm test -- --coverage --watchAll=false",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Suri by Chat Bot Maker",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@capacitor/app": "1.1.1",
    "@capacitor/core": "3.5.0",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@faker-js/faker": "6.2.0",
    "@ionic/react": "6.0.0",
    "@ionic/react-router": "6.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.6.3",
    "@types/axios": "0.14.0",
    "@types/history": "^5.0.0",
    "@types/jest": "26.0.20",
    "@types/node": "12.19.15",
    "@types/react": "16.14.3",
    "@types/react-dom": "16.9.10",
    "@types/react-router": "5.1.11",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^28.0.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-import-helpers": "^1.2.1",
    "git-commit-msg-linter": "4.1.2",
    "husky": "7.0.4",
    "ionicons": "5.4.0",
    "jest": "27.5.1",
    "jest-transform-css": "^4.0.1",
    "lint-staged": "12.4.1",
    "node-sass": "7.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.0",
    "ts-jest": "27.1.4",
    "typescript": "^4.1.3",
    "web-vitals": "0.2.4",
    "workbox-background-sync": "5.1.4",
    "workbox-broadcast-update": "5.1.4",
    "workbox-cacheable-response": "5.1.4",
    "workbox-core": "5.1.4",
    "workbox-expiration": "5.1.4",
    "workbox-google-analytics": "5.1.4",
    "workbox-navigation-preload": "5.1.4",
    "workbox-precaching": "5.1.4",
    "workbox-range-requests": "5.1.4",
    "workbox-routing": "5.1.4",
    "workbox-strategies": "5.1.4",
    "workbox-streams": "5.1.4"
  },
  "eslintConfig": {
    "plugins": [
      "eslint-plugin-import-helpers"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "**/*.scss"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "import-helpers/order-imports": [
        "warn",
        {
          "newlinesBetween": "always",
          "groups": [
            [
              "parent",
              "sibling",
              "index"
            ],
            "/^src/",
            "module"
          ],
          "alphabetize": {
            "order": "asc",
            "ignoreCase": true
          }
        }
      ],
      "@typescript-eslint/no-redeclare": "off",
      "jsx-a11y/anchor-is-valid": "off",
      "testing-library/no-node-access": "off"
    }
  },
  "dependencies": {
    "-": "^0.0.1",
    "@capacitor/cli": "3.5.0",
    "@capacitor/core": "3.5.0",
    "@ionic/core": "6.1.3",
    "@ionic/react": "6.1.3",
    "@ionic/react-router": "6.1.3",
    "D": "^1.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "recoil": "^0.7.3-alpha.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{ts,tsx}",
      "!./src/main/**/*",
      "!./src/**/index.{ts,js,tsx}",
      "!./**/*.d.ts"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(s?)css$": "identity-obj-proxy",
      "src/(.*)": "<rootDir>/src/$1"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)+(spec).{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
