{
  "name": "portal-suri",
  "version": "0.0.0",
  "description": "This project is for testing chatbotmaker platform with news patterns",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "ionic serve",
    "build": "ionic build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)' --passWithNoTests --no-cache --runInBand",
    "eject": "react-scripts eject",
    "test:all": "npm run test -- --watchAll",
    "test:staged": "npm run test -- --watchAll=false --env=jsdom --findRelatedTests",
    "test:ci": "npm test -- --coverage --watchAll=false",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Suri by Chat Bot Maker",
  "license": "ISC",
